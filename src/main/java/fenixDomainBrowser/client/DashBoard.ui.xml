<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:fenixDomainBrowser.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:SplitLayoutPanel ui:field="delta" width="100%"
		height="100%">
		<g:west size="300">
			<f:ClassSearch ui:field="search">
			</f:ClassSearch>
		</g:west>
		<g:center>
			<g:ScrollPanel>
				<g:HTML>
					<div id='loc' style="margin:20px"></div>
				</g:HTML>
			</g:ScrollPanel>
		</g:center>
		<g:north size="150">
			<f:ClassSelected ui:field="selected"></f:ClassSelected>
		</g:north>
	</g:SplitLayoutPanel>
</ui:UiBinder> 